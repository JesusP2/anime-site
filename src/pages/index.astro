---
import MainLayout from "@/layout/main.astro";
import { Carousel } from "@/components/carousel";
import { jikanClient } from "@/lib/api/jikan.client";

console.log(Astro.url.searchParams.get("page"));
const res = await jikanClient.GET("/seasons/now", {
  params: {
    query: {
      page: 1,
    },
  },
});
const res2 = await jikanClient.GET("/top/anime", {
  params: {
    query: {
      filter: "airing",
      limit: 25,
    },
  },
});
console.log(res2.data?.data);
---

<MainLayout title="title">
  <Carousel animes={res?.data?.data} client:load />
</MainLayout>
